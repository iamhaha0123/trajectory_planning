<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>lrmate_200id</name>
  <version>1.1.0</version>
  <description>This package contains configuration data, 3D models and launch files
for LRMate-200iD robot.</description>
  <maintainer email="ubuntu@gmail.com">ubuntu</maintainer>

  <license>BSD 3-clause</license>
  <url>https://github.com/mattj23/lrmate-200id</url>
  <author email="ubuntu@gmail.com">ubuntu</author>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <exec_depend>urdf_launch</exec_depend>

  <test_depend>ament_lint_auto</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
    <gazebo_ros gazebo_model_path="${prefix}/.."/>
  </export>
</package>
